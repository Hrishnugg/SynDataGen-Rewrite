{
  "name": "synoptic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:emulator": "firebase emulators:exec --only firestore,auth \"jest\"",
    "emulators:start": "firebase emulators:start --only firestore,auth",
    "emulators:stop": "firebase emulators:stop",
    "test:data-generation": "jest src/tests/lib/services/data-generation",
    "migrate": "ts-node --project tsconfig.scripts.json src/scripts/migration/migrateAll.ts",
    "migrate:customers": "ts-node --project tsconfig.scripts.json src/scripts/migration/migrateCustomers.ts",
    "migrate:waitlist": "ts-node --project tsconfig.scripts.json src/scripts/migration/migrateWaitlist.ts",
    "migrate:dry-run": "ts-node --project tsconfig.scripts.json src/scripts/migration/migrateAll.ts --dry-run",
    "firestore:deploy-rules": "firebase deploy --only firestore:rules",
    "firestore:deploy-indexes": "firebase deploy --only firestore:indexes",
    "firestore:test": "ts-node --project tsconfig.scripts.json src/scripts/firestore-test.ts",
    "firestore:test:js": "node src/scripts/firestore-test.js",
    "firestore:create-test-data": "ts-node --project tsconfig.scripts.json src/scripts/create-test-data.ts",
    "firestore:optimize-test": "ts-node --project tsconfig.scripts.json src/scripts/test-firestore-optimizations.ts",
    "test:service-account": "ts-node --project tsconfig.scripts.json src/scripts/test-service-account.ts",
    "backup:firestore": "ts-node --project tsconfig.scripts.json src/lib/gcp/firestore/backup-cli.ts backup",
    "restore:firestore": "ts-node --project tsconfig.scripts.json src/lib/gcp/firestore/backup-cli.ts restore",
    "test:firestore-api": "ts-node --project tsconfig.scripts.json src/scripts/test-firestore-api.ts",
    "test:waitlist-dual-write": "echo \"[DEPRECATED] Dual-write mode is no longer supported\" && exit 1",
    "test:mongodb": "echo \"[DEPRECATED] MongoDB is no longer supported\" && exit 1",
    "validate:data": "echo \"[DEPRECATED] MongoDB validation is no longer needed\" && exit 1",
    "backend:status": "echo \"[INFO] All collections are using Firestore\"",
    "backend:set": "echo \"[DEPRECATED] Backend selection is no longer supported - all collections use Firestore\"",
    "migrate:complete": "echo \"[INFO] Migration is already complete - all collections use Firestore\"",
    "migration-control": "echo \"[DEPRECATED] Migration control is no longer needed - migration is complete\""
  },
